<Page x:Class="PooP.GUI.Views.MainMenu.MainMenuInterface"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:l="clr-namespace:PooP.GUI.Views.MainMenu"
      mc:Ignorable="d" 
      Title="Main menu">
    <Canvas Width="800" Height="600">
        <Canvas.Background>
            <ImageBrush ImageSource="/SmallWorld;component/images/mainMenu.png" />
        </Canvas.Background>
        <Button Content="Button" Command="{Binding NewGame}" Canvas.Left="372" Canvas.Top="155"/>
    </Canvas>
</Page>
